{
    "name": "Cornell Extension",
    "description": "This Extension is form implement method cornell.",
    "author": "fray.ley@gmail.com",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_title": "Press Shift+C",
        "default_popup": "index.html"
    },
    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "automation": {
        "desktop": true
    },
    "permissions": [
        "activeTab",
        "scripting",
        "storage"
    ]
}